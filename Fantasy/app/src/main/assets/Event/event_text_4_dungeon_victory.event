<Event name="test_event_4_dungeon_victory">
	<EventCondition>
		<Conditions mode="and">
			<Condition type1="global" value1="0" mnemonic="==" type2="const" value2="13"/>
			<Condition type1="system" value1="0" mnemonic="==" type2="const" value2="7"/>
			<Condition type1="global" value1="127" mnemonic="==" type2="const" value2="4"/>
